{
  "name": "react-coroutine",
  "version": "2.0.0",
  "description": "React Components as Coroutines",
  "author": "Alexey Raspopov",
  "license": "MIT",
  "homepage": "https://react-coroutine.js.org/",
  "repository": "alexeyraspopov/react-coroutine",
  "main": "build/react-coroutine.js",
  "module": "build/react-coroutine.module.js",
  "files": [
    "build/",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "keywords": [
    "async",
    "react",
    "component",
    "container",
    "coroutine",
    "generator",
    "stateful",
    "iterator"
  ],
  "scripts": {
    "test": "jest --coverage",
    "ci": "sh scripts/integration.sh",
    "prepublish": "rollup --config scripts/rollup.config.js"
  },
  "peerDependencies": {
    "react": "~16.2.0"
  },
  "devDependencies": {
    "babel-core": "~6.26.0",
    "babel-jest": "~22.4.1",
    "babel-plugin-external-helpers": "~6.22.0",
    "babel-preset-env": "~1.6.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-3": "~6.24.1",
    "enzyme": "~3.3.0",
    "enzyme-adapter-react-16": "~1.1.1",
    "enzyme-to-json": "~3.3.1",
    "jest": "~22.4.2",
    "jest-junit": "~3.6.0",
    "react": "~16.2.0",
    "react-dom": "~16.2.0",
    "react-test-renderer": "~16.2.0",
    "rollup": "~0.56.5",
    "rollup-plugin-babel": "~3.0.3",
    "size-limit": "~0.16.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "./scripts/setup.jest.js",
    "collectCoverageFrom": [
      "modules/*.js"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "8.9.4"
          }
        }
      ],
      "react",
      "stage-3"
    ]
  },
  "size-limit": [
    {
      "limit": "765 B",
      "path": "build/react-coroutine.js"
    },
    {
      "limit": "770 B",
      "path": "build/react-coroutine.module.js"
    }
  ]
}
