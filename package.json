{
  "name": "react-coroutine",
  "version": "2.0.0",
  "description": "React Components as Coroutines",
  "main": "lib/Coroutine.js",
  "module": "lib/Coroutine.module.js",
  "files": [
    "lib/",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "test": "jest",
    "size": "size-limit",
    "buildcjs": "rollup --format cjs --config rollup.config.js modules/Coroutine.js > lib/Coroutine.js",
    "buildesm": "rollup --format es --config rollup.config.js modules/Coroutine.js > lib/Coroutine.module.js",
    "prepublish": "npm run buildcjs && npm run buildesm"
  },
  "homepage": "https://react-coroutine.js.org/",
  "repository": "alexeyraspopov/react-coroutine",
  "keywords": [
    "async",
    "react",
    "component",
    "container",
    "coroutine",
    "generator",
    "stateful",
    "iterator"
  ],
  "author": "Alexey Raspopov",
  "license": "MIT",
  "dependencies": {
    "shallowequal": "~1.0.1"
  },
  "peerDependencies": {
    "react": "~16.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.x",
    "babel-core": "6.x",
    "babel-jest": "15.x",
    "babel-plugin-external-helpers": "6.x",
    "babel-polyfill": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-es2017": "6.x",
    "babel-preset-react": "6.x",
    "babel-preset-stage-3": "6.x",
    "jest-cli": "15.x",
    "react-test-renderer": "~16.2.0",
    "rollup": "0.41.x",
    "rollup-plugin-babel": "2.x",
    "size-limit": "~0.16.1"
  },
  "size-limit": [
    {
      "limit": "1.08 KB",
      "path": "lib/Coroutine.js"
    }
  ]
